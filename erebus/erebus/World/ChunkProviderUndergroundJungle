            int j5 = j + hellRNG.nextInt(16) + 8;
            if(worldObj.getBlockId(j2, l3, j5) == 0 && worldObj.getBlockId(j2, l3 - 1, j5) == Block.grass.blockID)
            {
                (new WorldGenErebusTrees(true, 5, 0, 0, true, mod_Erebus.woodMahogany.blockID, mod_Erebus.leavesMahogany.blockID)).generate(worldObj, hellRNG, j2, l3, j5);
            }
